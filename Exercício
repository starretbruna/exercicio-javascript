}

55:
JS while loops

let i = 0;
while (i < 10) {
  console.log(i);
  i++
}

56:

let i = 0;
while (i < 10) {
  console.log(i);
i = i + 2; 
}

57:
JS break loops

for (i = 0; i < 10; i++) {
  console.log(i);
  if (i == 5) {
    
break;
   }
}

58:
for (i = 0; i < 10; i++) {
  if (i == 5) {
    
continue;
  }
  console.log(i);
}


